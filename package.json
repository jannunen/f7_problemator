{
  "name": "Problemator",
  "private": true,
  "version": "0.8.18",
  "description": "Problemator is an app for tracking your climbing",
  "repository": "",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production vite build && npx workbox generateSW workbox-config.js",
    "builddev": "cross-env NODE_ENV=development vite build && npx workbox generateSW workbox-config.js",
    "postinstall": "cpy ./node_modules/framework7-icons/fonts/*.* ./src/fonts/ && cpy ./node_modules/material-icons/iconfont/*.* ./src/fonts/",
    "deploy": "aws s3 sync ./www s3://problematorpws --delete && aws cloudfront create-invalidation --distribution-id EA0UQAAMYCBH9 --paths '/*'",
    "lint": "eslint --ext .js,.jsx ."
  },
  "browserslist": [
    "Android >= 7",
    "IOS >= 11",
    "Safari >= 11",
    "Chrome >= 49",
    "Firefox >= 31",
    "Samsung >= 5"
  ],
  "dependencies": {
    "@auth0/auth0-vue": "^1.0.2",
    "@tailwindcss/forms": "^0.5.3",
    "axios": "^0.26.1",
    "dayjs": "^1.11.0",
    "dom7": "^3.0.0",
    "dotenv": "^16.0.3",
    "framework7": "^6.3.4",
    "framework7-icons": "^5.0.3",
    "framework7-vue": "^6.3.16",
    "lodash": "^4.17.21",
    "material-icons": "^1.2.1",
    "rxjs": "^7.5.5",
    "skeleton-elements": "^3.5.0",
    "swiper": "^6.8.4",
    "tailwindcss": "^3.0.23",
    "v-calendar": "^3.0.0-alpha.8",
    "vite-plugin-package-version": "^1.0.2",
    "vue": "^3.2.40",
    "vue-chart-3": "^3.1.8",
    "vue-chartjs": "^4.1.2",
    "vue-i18n": "^9.2.0-beta.33",
    "vue3-qrcode-reader": "^0.0.1",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.0",
    "@vitejs/plugin-basic-ssl": "^0.1.2",
    "@vitejs/plugin-vue": "^3.1.2",
    "autoprefixer": "^10.4.4",
    "cpy-cli": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "less": "^4.1.1",
    "postcss": "^8.4.12",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.0",
    "rollup-plugin-framework7": "^1.2.0",
    "vite": "^3.1.8",
    "vite-plugin-environment": "^1.1.2",
    "workbox-cli": "^6.3.0"
  }
}
